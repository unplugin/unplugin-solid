{
  "name": "unplugin-solid",
  "version": "0.1.0",
  "packageManager": "pnpm@8.6.9",
  "author": "Ray <i@mk1.io> (@so1ve)",
  "description": "",
  "keywords": [
    "unplugin",
    "astro",
    "vite",
    "webpack",
    "rollup",
    "transform"
  ],
  "homepage": "https://github.com/so1ve/unplugin-solid#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/so1ve/unplugin-solid.git"
  },
  "bugs": {
    "url": "https://github.com/so1ve/unplugin-solid/issues"
  },
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./*": "./*",
    "./astro": {
      "types": "./dist/astro.d.ts",
      "require": "./dist/astro.cjs",
      "import": "./dist/astro.mjs"
    },
    "./esbuild": {
      "types": "./dist/esbuild.d.ts",
      "require": "./dist/esbuild.cjs",
      "import": "./dist/esbuild.mjs"
    },
    "./rollup": {
      "types": "./dist/rollup.d.ts",
      "require": "./dist/rollup.cjs",
      "import": "./dist/rollup.mjs"
    },
    "./rspack": {
      "types": "./dist/rspack.d.ts",
      "require": "./dist/rspack.cjs",
      "import": "./dist/rspack.mjs"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "require": "./dist/types.cjs",
      "import": "./dist/types.mjs"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "require": "./dist/vite.cjs",
      "import": "./dist/vite.mjs"
    },
    "./webpack": {
      "types": "./dist/webpack.d.ts",
      "require": "./dist/webpack.cjs",
      "import": "./dist/webpack.mjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "unbuild",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "nr build",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "stub": "unbuild --stub",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@babel/core": "^7.20.5",
    "@babel/preset-typescript": "^7.18.6",
    "@rollup/pluginutils": "^5.0.2",
    "babel-preset-solid": "^1.7.2",
    "merge-anything": "^5.1.4",
    "solid-refresh": "^0.5.0",
    "unplugin": "^1.3.1",
    "vitefu": "^0.2.3"
  },
  "devDependencies": {
    "@antfu/ni": "^0.21.3",
    "@so1ve/eslint-config": "^0.102.0",
    "@types/babel__core": "^7.1.20",
    "@types/node": "^18.16.6",
    "bumpp": "^9.1.0",
    "eslint": "^8.40.0",
    "prettier": "^2.8.8",
    "rollup": "^3.21.6",
    "typescript": "^5.0.4",
    "unbuild": "^1.2.1",
    "vite": "^4.3.5",
    "vitest": "^0.32.0",
    "webpack": "^5.82.0"
  },
  "peerDependencies": {
    "solid-js": "^1.7.2"
  }
}
